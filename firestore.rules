
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are for development and testing only.
    // They allow any authenticated user to read and write to the entire database.
    // DO NOT use these rules in a production environment.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
