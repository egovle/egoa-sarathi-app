rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // WARNING: These rules are insecure and are for development testing only.
    // They allow anyone to read and write to your entire storage.
    // Replace with secure rules before going to production.
    match /{allPaths=**} {
      allow read, write: if true;
    }
  }
}
